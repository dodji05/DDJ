{% extends 'FrontEnd/base_page_interieur.html.twig' %}
{% block stylesheet %}
    <link rel="stylesheet" href=" {{ asset('FrontEnd/assets/css/form-main.css') }}">
    <link rel="stylesheet" href=" {{ asset('FrontEnd/assets/css/form-util.css') }}">
{% endblock %}
 {% block leftcontent %}
     <div class="limiter">
         <div class="container-login100">
             <div class="wrap-login100">
                 <form class="login100-form validate-form p-l-55 p-r-55 p-t-178">
					<span class="login100-form-title">
						 Exprimez vous !
					</span>



                     <div class="col-md-12 form-item mb-5">
{#                         <label for="nom" class="form-label">Nom  </label>#}
                         <input type="text"  id="inputNom" class="form-control" name="nom" id="nom" placeholder="Nom" autocomplete="nom" />
                     </div>

                     <div class="col-md-12 form-item mb-5">
{#                         <label for="email" class="form-label">E-mail </label>#}
                         <input type="email"  id="inputEmail" class="form-control" name="email" id="email" placeholder="E-mail" autocomplete="email" required autofocus/>
                     </div>

                     <div class="col-md-12 form-item mb-5">
                         <input class="form-control" type="file" id="formFile">
                     </div>


                     <div class="container-login100-form-btn">
                         <button class="login100-form-btn abonnement-link abonnement--premium button-edito">
                             Envoyez mon dessin
                         </button>
                     </div>

                     <div class="flex-col-c p-t-170 p-b-40">
						<span class="txt1 p-b-9">
<!--							Donâ€™t have an account?-->
						</span>

                         <a href="#" class="txt3">
                             <!--                                            Sign up now-->
                         </a>
                     </div>
                 </form>
             </div>
         </div>
     </div>
 {% endblock %}
